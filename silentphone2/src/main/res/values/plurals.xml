<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

  <plurals name="notify_new_messages_title">
    <item quantity="zero"><xliff:g id="quantity">%1$d</xliff:g> new messages</item>
    <item quantity="one">New message received</item>
    <item quantity="two"><xliff:g id="quantity">%1$d</xliff:g> new messages</item>
    <item quantity="few"><xliff:g id="quantity">%1$d</xliff:g> new messages</item>
